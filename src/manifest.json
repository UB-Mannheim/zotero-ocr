{
  "manifest_version": 2,
  "name": "Zotero OCR",
  "version": "0.7.0",
  "description": "Zotero Plugin for OCR",
  "author": "Philipp Zumstein",
  "homepage_url": "https://github.com/UB-Mannheim/zotero-ocr",
  "icons": {
    "48": "chrome/skin/default/zoteroocr/ubmannheim48x48.png",
    "96": "chrome/skin/default/zoteroocr/ubmannheim96x96.png"
  },
  "applications": {
    "zotero": {
      "id": "zotero-ocr@uni-mannheim.de",
      "update_url": "https://raw.githubusercontent.com/UB-Mannheim/zotero-ocr/master/update.json",
      "strict_min_version": "6.999",
      "strict_max_version": "7.0.*"
    }
  }
}
